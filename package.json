{
  "name": "ant-design-pro",
  "version": "1.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "cross-env MOCK=none umi build",
    "fetch:blocks": "pro fetch-blocks && npm run prettier",
    "format-imports": "cross-env import-sort --write '**/*.{js,jsx,ts,tsx}'",
    "gh-pages": "cp CNAME ./dist/ && gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "ui": "umi ui",
    "ws": "node ws.js",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.{js,ts,tsx}": "npm run lint-staged:js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/colors": "^3.1.0",
    "@ant-design/icons": "^4.0.3",
    "@ant-design/pro-layout": "^4.5.16",
    "@antv/data-set": "^0.10.2",
    "@types/crypto-js": "^3.1.43",
    "@types/js-cookie": "^2.2.4",
    "@types/object-path": "^0.11.0",
    "@types/react-router": "^5.0.2",
    "antd": "^3.23.6",
    "antd-img-crop": "^2.1.2",
    "benz-amr-recorder": "^1.1.2",
    "braft-editor": "^2.3.8",
    "classnames": "^2.2.6",
    "cropperjs": "^1.5.6",
    "crypto-js": "^3.1.9-1",
    "dva": "^2.4.1",
    "express": "^4.17.1",
    "history": "^4.10.1",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.2.1",
    "libphonenumber-js": "^1.7.26",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "moment-range": "^4.0.2",
    "object-path": "^0.11.4",
    "omit.js": "^1.0.2",
    "path-to-regexp": "^3.1.0",
    "prop-types": "^15.7.2",
    "qiniu-js": "^2.5.5",
    "qrcode": "^1.3.3",
    "qrcode.react": "^0.9.3",
    "qs": "^6.9.0",
    "react": "^16.8.6",
    "react-copy-to-clipboard": "^5.0.1",
    "react-cropper": "^1.3.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.6",
    "react-image-crop": "^8.4.2",
    "react-infinite-scroller": "^1.2.4",
    "react-player": "^2.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^5.1.2",
    "react-zmage": "^0.8.5-beta.31",
    "redux": "^4.0.1",
    "save": "^2.4.0",
    "slash2": "^2.0.0",
    "socket.io-client": "^2.3.0",
    "umi": "^2.9.6",
    "umi-plugin-pro-block": "^1.3.4",
    "umi-plugin-react": "^1.10.1",
    "umi-request": "^1.2.7",
    "video-react": "^0.14.1",
    "webpack-theme-color-replacer": "^1.2.15"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^1.0.13",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@commitlint/format": "^8.2.0",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.144",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.8.19",
    "@types/react-document-title": "^2.0.3",
    "@types/react-dom": "^16.8.4",
    "@types/react-infinite-scroller": "^1.2.1",
    "@umijs/fabric": "^1.2.0",
    "chalk": "^2.4.2",
    "check-prettier": "^1.0.3",
    "commitizen": "^4.0.3",
    "cross-env": "^6.0.0",
    "cross-port-killer": "^1.1.1",
    "cz-conventional-changelog": "^3.0.2",
    "enzyme": "^3.9.0",
    "eslint": "^5.16.0",
    "gh-pages": "^2.0.1",
    "husky": "^3.0.2",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest-puppeteer": "^4.2.0",
    "lint-staged": "^9.2.3",
    "mockjs": "^1.0.1-beta3",
    "node-fetch": "^2.6.0",
    "postcss": "^7.0.23",
    "prettier": "^1.18.2",
    "pro-download": "1.0.1",
    "socket.io": "^2.3.0",
    "stylelint": "^10.1.0",
    "umi-plugin-ga": "^1.1.3",
    "umi-plugin-pro": "^1.0.2",
    "umi-types": "^0.5.0"
  },
  "optionalDependencies": {
    "puppeteer": "^1.17.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ],
  "peerDependencies": {
    "@ant-design/pro-layout": "^4.5.16"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
